{
  "compilerOptions": {
    "baseUrl": "./",
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es5",
    "declaration": true,
    "esModuleInterop": true,
    "lib": [
      "es2017",
      "es7",
      "es6",
      "dom"
    ],
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "moduleResolution": "node"
  }
}
